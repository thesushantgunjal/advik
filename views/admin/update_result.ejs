<%-include("navbar.ejs")%>

<form action="/admin/result_update_data" method="post" enctype="multipart/form-data">
    <div class="container-fluid bg-white p-4">
        <div class="row">
            <div class="col-md-12">
                <h3 class="heading">Result Update</h3>
            </div>
        </div>

        <div class="row">
            <!-- Dropdown selection for category -->
            
            
            <!-- Input for new category name -->
            <div class="col-md-6 mb-3">
                <label for="category_name">gallry img</label>
                <input type="file" id="gallery_img" name="result_img" class="form-control">
            </div>

            <div class="col-md-6 mb-3">
                <img src="/uploads/<%-old_result_data.result_img%>" alt="" width="100px" height="ms-auto">
            </div>
           
            <div class="col-md-6 mb-3">
                <input type="hidden" name="result_update_data" value="<%-old_result_data.result_id%>" >
                <label for="category_name">Student Name </label>
                <input type="text" value="<%-old_result_data.student_name%>" id="category_name" name="student_name" class="form-control">
            </div>

            <div class="col-md-6 mb-3">
                <label for="category_name"> parentage </label>
                <input type="text" value="<%-old_result_data.student_parentage%>"  name="student_parentage" class="form-control">
            </div>
           
        </div>

        <div class="row">
            <div class="col-md-12 mb-3 mt-5 text-center">
                <button type="submit" class="btn btn-primary">Save </button>
            </div>
        </div>
    </div>
</form>


<%-include("footer.ejs")%>